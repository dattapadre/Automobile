 <%-include('navbar.ejs')%>
 <div class="container p-5">
        <div class="row">
            <div class="col-md-12 shadow-lg p-3">
                <form action="/admin/update_type" method="post" enctype="multipart/form-data">
                <div class="row ">
                    <h3 class="mb-3">Edit Menu</h3>
                    <div class="col-md-12">
                        <label class="form-label" for="name">Menu Type</label>
                        <input type="text" name="typename" class="form-control" required value="<%-result[0].type_name%>">
                        <input type="hidden" name="id" class="form-control" required value="<%-result[0].type_id%>">
                    </div>
                </div>
                
                <div class="row mt-4">
                    <div class="col-md-12">
                        <button class="btn btn-primary text-center ps-3 ">Save</button>
                    </div>
                </div>
                </form>
            </div>
        </div>
    </div>
    <%-include('footer.ejs')%>